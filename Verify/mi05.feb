<?xml version="1.0" encoding="ISO-8859-1"?>
<!--
Two element structure.  Node 1 is constrained in x,y,z.  Node 12 has an x-displacement of
1 unit at time t=1.  The z-displacement of node 10 is the negative of the x-displacement
of node 12.  Node 3 is constrained to move only in the y-direction.
-->
<febio_spec version="3.0">
	<Module type="solid"/>
	<Control>
		<analysis>STATIC</analysis>
		<time_steps>10</time_steps>
		<step_size>0.1</step_size>
		<solver>
			<max_refs>15</max_refs>
			<max_ups>10</max_ups>
			<diverge_reform>1</diverge_reform>
			<reform_each_time_step>1</reform_each_time_step>
			<dtol>0.001</dtol>
			<etol>0.01</etol>
			<rtol>0</rtol>
			<lstol>0.9</lstol>
			<min_residual>1e-20</min_residual>
			<qnmethod>BFGS</qnmethod>
			<rhoi>-2</rhoi>
		</solver>
		<time_stepper>
			<dtmin>0.01</dtmin>
			<dtmax>0.1</dtmax>
			<max_retries>5</max_retries>
			<opt_iter>10</opt_iter>
		</time_stepper>
	</Control>
	<Globals>
		<Constants>
			<T>0</T>
			<R>0</R>
			<Fc>0</Fc>
		</Constants>
	</Globals>
	<Material>
		<material id="1" name="Material 1" type="neo-Hookean">
			<density>1</density>
			<E>1</E>
			<v>0</v>
		</material>
	</Material>
	<Mesh>
		<Nodes name="Object01">
			<node id="1">-1,-0.5,0</node>
			<node id="2">-1,-0.5,1</node>
			<node id="3">-1,0.5,0</node>
			<node id="4">-1,0.5,1</node>
			<node id="5">0,-0.5,0</node>
			<node id="6">0,-0.5,1</node>
			<node id="7">0,0.5,0</node>
			<node id="8">0,0.5,1</node>
			<node id="9">1,-0.5,0</node>
			<node id="10">1,-0.5,1</node>
			<node id="11">1,0.5,0</node>
			<node id="12">1,0.5,1</node>
		</Nodes>
		<Elements type="hex8" name="Part9">
			<elem id="1">1,5,7,3,2,6,8,4</elem>
			<elem id="2">5,9,11,7,6,10,12,8</elem>
		</Elements>
		<NodeSet name="FixedDisplacement01">
			<n id="1"/>
			<n id="3"/>
			<n id="12"/>
		</NodeSet>
		<NodeSet name="FixedDisplacement02">
			<n id="1"/>
			<n id="12"/>
		</NodeSet>
		<NodeSet name="FixedDisplacement03">
			<n id="1"/>
			<n id="3"/>
		</NodeSet>
		<NodeSet name="PrescribedBC155">
			<n id="12"/>
		</NodeSet>
		<ElementSet name="unnamed_set01">
			<e id="2"/>
		</ElementSet>
	</Mesh>
	<MeshDomains>
		<SolidDomain name="Part9" mat="Material 1"/>
	</MeshDomains>
	<Boundary>
		<bc name="FixedDisplacement01" type="fix" node_set="FixedDisplacement01">
			<dofs>z</dofs>
		</bc>
		<bc name="FixedDisplacement02" type="fix" node_set="FixedDisplacement02">
			<dofs>y</dofs>
		</bc>
		<bc name="FixedDisplacement03" type="fix" node_set="FixedDisplacement03">
			<dofs>x</dofs>
		</bc>
		<bc name="PrescribedBC155" type="prescribe" node_set="PrescribedBC155">
			<dof>x</dof>
			<scale lc="1">1</scale>
			<relative>0</relative>
		</bc>
		<bc type="linear constraint">
			<node>10</node>
			<dof>z</dof>
			<child_dof>
				<node>12</node>
				<dof>x</dof>
				<value>-1</value>
			</child_dof>
		</bc>
	</Boundary>
	<LoadData>
		<load_controller id="1" type="loadcurve">
			<interpolate>LINEAR</interpolate>
			<points>
				<point>0,0</point>
				<point>1,1</point>
			</points>
		</load_controller>
	</LoadData>
	<Output>
		<plotfile type="febio">
			<var type="displacement"/>
			<var type="relative volume"/>
			<var type="stress"/>
		</plotfile>
		<logfile>
			<element_data data="sx" elem_set="unnamed_set01"/>
		</logfile>
	</Output>
</febio_spec>
